{
  "name": "artusx-koa-bench",
  "version": "1.0.0",
  "description": "koa for artusx",
  "main": "index.js",
  "scripts": {
    "bench:fmy": "cd bench && BENCH_FIND_MY_WAY=true make",
    "bench:koa-router": "cd bench && BENCH_KOA_ROUTER=true make",
    "build": "",
    "ci": "npm run lint",
    "dev": "ARTUS_SERVER_ENV=development npx nodemon src/index.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "start": "node dist/index.js",
    "tsc": "rm -rf dist && tsc"
  },
  "dependencies": {
    "@artusx/core": "workspace:*",
    "@artusx/utils": "workspace:*",
    "cron": "^2.2.0",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "fs-extra": "~11.2.0"
  },
  "devDependencies": {
    "@artusx/eslint-config": "workspace:*",
    "@artusx/tsconfig": "workspace:*",
    "@types/fs-extra": "~11.0.4",
    "@types/node": "~20.10.6",
    "@typescript-eslint/eslint-plugin": "~6.19.1",
    "eslint": "~8.56.0",
    "eslint-plugin-import": "~2.29.1",
    "nodemon": "~3.0.2",
    "reflect-metadata": "^0.1.13",
    "ts-node": "~10.9.2",
    "tslib": "~2.6.2",
    "typescript": "~5.3.3"
  }
}
