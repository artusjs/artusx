{
  "name": "artusx-worker",
  "version": "0.0.0-dev.0",
  "description": "worker app powered by artusx",
  "keywords": [
    "artusx"
  ],
  "homepage": "https://github.com/artusjs/artusx",
  "repository": {
    "type": "git",
