    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@artusx/eslint-config": "workspace:*",
    "@artusx/tsconfig": "workspace:*",
    "@types/node": "^18.11.17",
    "@typescript-eslint/eslint-plugin": "~6.19.1",
    "eslint": "~8.56.0",
    "eslint-plugin-import": "~2.29.1",
    "nodemon": "~3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
