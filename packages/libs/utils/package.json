{
  "name": "@artusx/utils",
  "version": "1.0.41",
  "description": "core utils for artusx",
  "keywords": [
    "artusx"
  ],
  "license": "MIT",
  "author": "Suyi <thonatos.yang@gmail.com>",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./application": {
      "types": "./lib/application.d.ts",
      "default": "./lib/application.js"
    },
    "./bootstrap": {
      "types": "./lib/bootstrap.d.ts",
      "default": "./lib/bootstrap.js"
    },
    "./utils": {
      "types": "./lib/utils.d.ts",
      "default": "./lib/utils.js"
    },
    "./constants": {
      "types": "./lib/constants.d.ts",
      "default": "./lib/constants.js"
    }
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "npm run tsc",
    "tsc": "rm -rf lib && tsc"
  },
  "dependencies": {
    "@artus/core": "^2.x",
    "dotenv": "~16.4.5"
  },
  "devDependencies": {
    "@artusx/tsconfig": "workspace:*",
    "@types/node": "^18.11.17",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "publishConfig": {
    "access": "public"
  }
}
